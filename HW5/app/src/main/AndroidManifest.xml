<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inahum.theshawshankredemption">

    <uses-permission android:name="COARSE_LOCATION"></uses-permission>
    <uses-permission android:name="FINE_LOCATION"></uses-permission>
    <uses-permission android:name="STATE_NETWORK"></uses-permission>
    <uses-permission android:name="INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/note"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditScreen"></activity>
        <activity android:name=".EditDataActivity"></activity>
    </application>

</manifest>